{
	"name": "mrwhiskers-timeline-blog",
	"version": "1.0.0",
	"description": "A minimal timeline-style blog template built with SvelteKit 5",
	"author": {
		"name": "tankbottoms",
		"email": "tankbottoms@users.noreply.github.com",
		"url": "https://github.com/tankbottoms"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/tankbottoms/tankbottoms.github.io.git"
	},
	"private": true,
	"type": "module",
	"engines": {
		"node": ">=22.0.0 <23.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && vite build",
		"preview": "vite preview",
		"dev:bun": "bunx --bun vite dev",
		"build:bun": "svelte-kit sync && bunx --bun vite build",
		"preview:bun": "bunx --bun vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"generate:semantic-triples": "bun run scripts/generate-semantic-triples.ts",
		"ipfs": "bun run scripts/deploy-ipfs.ts",
		"ipfs:redirect": "bun run scripts/deploy-ipfs.ts redirect",
		"ipfs:test": "bun run scripts/deploy-ipfs.ts test",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:report": "playwright show-report",
		"check-links": "bun run scripts/check-links.ts",
		"check-links:skip-external": "bun run scripts/check-links.ts --skip-external",
		"postbuild": "bun run scripts/check-links.ts --skip-external",
		"download-ipfs-cache": "bun run scripts/download-ipfs-cache.ts",
		"download-ipfs-cache:check": "bun run scripts/download-ipfs-cache.ts --check"
	},
	"devDependencies": {
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/adapter-vercel": "^6.2.0",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@tailwindcss/typography": "^0.5.19",
		"@types/file-saver": "^2.0.7",
		"gray-matter": "^4.0.3",
		"mdsvex": "^0.11.2",
		"playwright": "^1.57.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"svelte": "^5.46.0",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^3.4.19",
		"typescript": "^5.9.3",
		"vite": "^5.4.21"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.958.0",
		"@vercel/analytics": "^1.6.1",
		"@vercel/speed-insights": "^1.3.1",
		"buffer": "^6.0.3",
		"docx": "^9.5.1",
		"file-saver": "^2.0.5",
		"github-slugger": "^2.0.0",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-raw": "^7.0.0",
		"rehype-slug": "^6.0.0",
		"rehype-stringify": "^10.0.1",
		"remark": "^15.0.1",
		"remark-frontmatter": "^5.0.0",
		"remark-gfm": "^4.0.1",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.2",
		"unist-util-visit": "^5.0.0"
	}
}
